{
  "name": "@cloud-cli/template-webapi-ts",
  "version": "1.0.0",
  "description": "Cloud Template WebApi With TS",
  "main": "dist/app.js",
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "ISC",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch '*.*' -e ts,json --exec ts-node src/app.ts",
    "build": "tsc",
    "serve": "tsc && cross-env NODE_ENV=production node dist/app.js",
    "prepare": "husky install",
    "protoc": "bash src/bin/protoc.sh",
    "pm2:dev-start": "tsc && cross-env NODE_ENV=development pm2 start ecosystem.config.js",
    "pm2:start": "tsc && cross-env NODE_ENV=production pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:reload": "pm2 reload ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:kill": "pm2 kill",
    "pm2:monit": "pm2 monit",
    "pm2:logs": "pm2 logs",
    "pm2:show": "pm2 show web-api"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.1",
    "abort-controller-x": "^0.4.3",
    "aws-sdk": "^2.1445.0",
    "axios": "^1.2.1",
    "base64-min": "^2.0.0",
    "crypto-js": "^4.1.1",
    "google-protobuf": "^3.21.2",
    "http-errors": "^2.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.1",
    "koa-joi-router": "^8.0.0",
    "koa-json": "^2.0.2",
    "koa-qs": "^3.0.0",
    "koa-router": "^12.0.0",
    "koa2-cors": "^2.0.6",
    "koa2-swagger-ui": "^5.9.0",
    "log4js": "^6.9.1",
    "long": "^5.2.3",
    "nice-grpc": "^2.1.5",
    "nice-grpc-common": "^2.0.2",
    "node-abort-controller": "^3.1.1",
    "path-to-regexp": "^6.2.1",
    "protobufjs": "^7.2.5",
    "qs": "^6.11.2",
    "swagger-jsdoc": "^6.2.8",
    "ts-error": "^1.0.6",
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/google-protobuf": "^3.15.6",
    "@types/http-errors": "^2.0.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/koa": "^2.13.8",
    "@types/koa-bodyparser": "^4.3.7",
    "@types/koa-joi-router": "^8.0.4",
    "@types/koa-json": "^2.0.20",
    "@types/koa-qs": "^2.0.0",
    "@types/koa-router": "^7.4.4",
    "@types/koa2-cors": "^2.0.2",
    "@types/mockjs": "^1.0.7",
    "@types/node": "^20.5.6",
    "@types/qs": "^6.9.7",
    "@types/swagger-jsdoc": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "grpc-tools": "^1.12.4",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "mockjs": "^1.1.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.156.7",
    "typescript": "^5.2.2"
  }
}
